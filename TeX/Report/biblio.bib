@article{Ballard.Basseville2005,
  author =	 "Ballard, P. and Basseville, S.",
  title =	 "{Existence and uniqueness for dynamical unilateral
                  contact with {C}oulomb friction: a model problem.}",
  language =	 "English",
  journal =	 "ESAIM, Math. Model. Numer. Anal. ",
  volume =	 39,
  number =	 1,
  pages =	 "59-77",
  year =	 2005,
  doi =		 {10.1051/m2an:2005004},
  abstract =	 "{Consider the following evolution problem
                  $$\align\ddot{U} +K\cdot U = F + R,\quad t\in
                  [0,T],&amp;\quad \text{(motion equation)};\\ U(0)
                  =U_{0},\quad \dot{U}^{+}(0)=V_{0}, &amp;\quad
                  \text{(initial condition)};\\ U_{N}\le 0,\quad
                  R_{N}\le 0,\quad U_{N}R_{N}=0,&amp; \quad
                  \text{(unilateral contact)};\\
                  \int_{[0,T]}[R_{T}\cdot(V-\dot{U}^{+}_{T}) - \mu
                  R_{N}(\vert V\vert -\vert \dot{U}^{+}_{T}\vert
                  )]\,dt\ge 0\quad \forall V\in C^{0}([0,T];\Bbb
                  R^{n-1}), &amp;\quad \text{(Coulomb friction)};\\
                  U_{N}(t) = 0 \Longrightarrow \dot{U}^{+}_{T}(t)=
                  -e\dot{U}^{-}_{T}(t),\quad t\in (0,T],&amp;\quad
                  \text{(impact law)}, \endalign$$ where $U\in \Bbb
                  R^{n},$ $F$ denotes a given integrable function of
                  $[0,T]$ into $\Bbb R^{n}$, (external force), $\mu$ a
                  given nonnegative real constant (friction
                  coefficient), $e\in [0,1]$ a real constant
                  (restitution coefficient) and $(U_{0},V_{0})$ some
                  initial condition, assumed to be compatible with the
                  unilateral constraint $$U_{0N}\le 0\quad
                  \text{and}\quad U_{0N} = 0 \Longrightarrow V_{0N}\le
                  0.  $$ Here, $K$ is the symmetric positive definite
                  stiffness matrix of order $n.$ For $X \in \Bbb
                  R^{n}$, $X_N$ denotes its first component (normal
                  component) and $X_{T}$ the vector of $\Bbb R^{n-1}$
                  formed by the $n-1$ last components of $X$
                  (tangential component). The authors prove the
                  existence and uniqueness of a solution to this
                  problem.}",
  reviewer =	 "{Oleg Filatov (Samara)}",
  keywords =	 "{unilateral dynamics with friction; existence and
                  uniqueness}",
  classmath =	 "{*34A60 (ODE with multivalued right-hand sides)
                  34A36 (Discontinuous equations) 74M10 (Friction)
                  74M15 (Contact) }",
}

